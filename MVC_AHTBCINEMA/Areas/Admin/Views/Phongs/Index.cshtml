@model IEnumerable<AHTBCinema_NHOM4_SD18301.Models.Phong>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SoPhong)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TrangThai)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SoLuongGhe)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SoPhong)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TrangThai)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SoLuongGhe)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdPhong">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdPhong">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdPhong">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
